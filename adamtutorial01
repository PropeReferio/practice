def lsttimes5(multilst):
    print(multilst)
    new_lst = []
    for el in multilst:
        new_lst.append(el*5) #This line adds each element to new_list
        print(new_lst)

lst = [1,4,9,16,20,'bleaaahh']
lst2 = [6,2,3]
lsttimes5(lst)
lsttimes5(lst2)

