def howdoesitwork(word1, word2, word3):
    print(f"I went to the {word1} and bought a {word2} for 3 {word3}.")

howdoesitwork('market', 'chicken', 'potatoes')
howdoesitwork('bowling alley', 'shoe', 'straws')
howdoesitwork('shoe', 'bowling alley', 'straws')



